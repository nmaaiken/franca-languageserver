{
    "name": "xtext-vscode-example",
    "displayName": "Xtext VS Code example",
    "description": "A simple DSL example for VS Code",
    "version": "0.0.28",
    "publisher": "itemis",
    "icon": "images/itemis.png",
    "license": "EPL-1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/itemis/xtext-languageserver-example.git"
    },
    "bugs": {
        "url": "https://github.com/itemis/xtext-languageserver-example/issues"
    },
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:mydsl",
        "onLanguage:franca"
    ],
    "main": "out/extension",
    "contributes": {
        "languages": [
            {
                "id": "fidl",
                "aliases": [
                    "franca",
                    "fidl"
                ],
                "extensions": [
                    ".fidl"
                ],
                "configuration": "./franca.configuration.json"
            }
        ],
        "grammars": [],
        "commands": [
            {
                "command": "franca.a.proxy",
                "title": "MyDsl Command A"
            },
            {
                "command": "franca.b",
                "title": "MyDsl Command B"
            }
        ]
    },
    "devDependencies": {
        "vscode": "^1.1.33",
        "typescript": "^3.3.400",
        "@types/node": "^11.13.2"
    },
    "dependencies": {
        "vscode-languageclient": "^5.2.1"
    },
    "scripts": {
        "prepublish": "node ./node_modules/vscode/bin/install && tsc -p ./src",
        "compile": "tsc -p ./src",
        "watch": "tsc -w -p ./src",
        "update-vscode": "node ./node_modules/vscode/bin/install"
    }
}
